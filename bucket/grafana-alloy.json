{
    "version": "1.11.3",
    "description": "A universal Conda package builder for Windows, macOS and Linux",
    "homepage": "https://github.com/grafana/alloy",
    "license": "Apache-2.0",
    "bin": [
        [
            "allow.exe",
            "alloy-windows-amd64.exe"
        ]
    ],
    "architecture": {
        "64bit": {
            "url": "https://github.com/grafana/alloy/releases/download/v1.11.3/alloy-windows-amd64.exe.zip",
            "bin": "alloy-windows-amd64.exe"
        }
    },
    "checkver": {
        "github": "https://github.com/grafana/alloy"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/grafana/alloy/releases/download/v$version/alloy-windows-amd64.exe.zip",
                "bin": "alloy-windows-amd64.exe"
            }
        }
    }
}
